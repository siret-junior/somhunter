{
  "name": "somhunter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "",
    "postinstall": "npx gulp postinstall && cd ui && npm install && cd .. && npm run build-ui",
    "start": "concurrently -k \"npm run server\" \"npm run ui\" --prefix ./ui/",
    "start:dev": "concurrently -k  \"npm run server\" \"npm run ui-dev\" --prefix ./ui/",
    "server": "node ./bin/www somhunter",
    "server:dev": "node ./bin/www somhunter",
    "ui": "serve -s ./ui/build",
    "ui:dev": "npm run start --prefix ./ui/",
    "prebuild-ui": "npx gulp cleanSymlinks",
    "build-ui": "npm run build --prefix ./ui/",
    "postbuild-ui": "npx gulp createSymlinks",
    "build": "node-gyp build && npm run build-ui",
    "rebuild": "node-gyp rebuild",
    "build:debug": "node-gyp build --debug",
    "rebuild:debug": "node-gyp rebuild --debug",
    "clean": "npx gulp clean"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "node-gyp": "^3.7.0"
  },
  "dependencies": {
    "all": "0.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "ejs": "^2.7.4",
    "eslint-config-prettier": "^6.11.0",
    "express": "^4.16.0",
    "express-session": "^1.17.1",
    "gyp": "^0.5.0",
    "http-errors": "^1.6.2",
    "loadash": "^1.0.0",
    "lodash": "^4.17.20",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "morgan": "^1.9.0",
    "mysql": "^2.18.1",
    "node-addon-api": "^1.7.2",
    "node-libcurl": "^2.1.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "session-file-store": "^1.4.0",
    "socket.io-client": "^2.3.0",
    "spdy": "^4.0.2",
    "winston": "^3.2.1"
  }
}
